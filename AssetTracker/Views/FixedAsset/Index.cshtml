@{
    ViewData["Title"] = "Index";
}

<h1>Lista zasobów</h1>

<button type="button" id="addAssetBtn" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#addAssetModal">Dodaj Zasób</button>

<partial name="_ModalsFixedAssetPartialView"/>

<table id="AssetTable" class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Nazwa
            </th>
            <th>
                Model
            </th>
            <th>
                Numer Seryjny
            </th>
            <th>
                Kod Zasobu
            </th>
            <th>
                Czy w użyciu
            </th>
            <th>
                Akcje
            </th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>

